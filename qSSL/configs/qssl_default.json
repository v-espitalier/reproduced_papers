{
  "seed": 42,
  "outdir": "outdir",
  "device": "cpu",
  "dataset": {
    "root": "./data",
    "classes": 5,
    "batch_size": 256
  },
  "model": {
    "backend": "merlin",
    "width": 8,
    "loss_dim": 128,
    "batch_norm": false,
    "temperature": 0.07,

    "modes": 10,
    "no_bunching": false,

    "layers": 2,
    "encoding": "vector",
    "q_ansatz": "sim_circ_14_half",
    "q_sweeps": 1,
    "activation": "null",
    "shots": 100
  },

  "training": {
    "epochs": 2,
    "ckpt_step": 1,
    "le_epochs": 100
  },
  "logging": {"level": "info"}
}
